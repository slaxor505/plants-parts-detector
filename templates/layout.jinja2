<body>

    <nav class="navbar is-link" role="navigation" aria-label="main navigation">
        <div class="navbar-brand">
<!--             <a class="navbar-item is-hoverable" href="/">
                 <img src="{{url_for('static', filename='img/axenet_icon_trans.png')}}" width="112" height="29">
             </a>
-->
            <a class="navbar-item is-hoverable" href="/">Home</a>
            <a class="navbar-item is-hoverable" href="https://plantbook.io">What is Plantbook</a>


            <a role="button" class="navbar-burger burger" aria-label="menu" aria-expanded="false" data-target="navbarBasicExample">
                <span aria-hidden="true"></span>
                <span aria-hidden="true"></span>
                <span aria-hidden="true"></span>
            </a>
        </div>

        <div id="navbarBasicExample" class="navbar-menu">
